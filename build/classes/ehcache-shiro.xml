<?xml version="1.0" encoding="UTF-8"?>

<ehcache updateCheck="false" name="hibernateCache">

	<diskStore path="java.io.tmpdir/ehcache/crm/shiro" />

	<defaultCache 
		maxElementsInMemory="10000" 
		eternal="false" 
		timeToIdleSeconds="120" 
		timeToLiveSeconds="120"
		overflowToDisk="true" />

</ehcache>
